<Window x:Class="IS_Bolnice.Prozori.Prozori_za_pacijenta.OdabirZakazivanja"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:IS_Bolnice.Prozori.Prozori_za_pacijenta"
        mc:Ignorable="d"
       Title="Zakazivanje pregleda" Height="300" Width="310"
        WindowStartupLocation="CenterScreen">
    <StackPanel>
        <!--Zakazivanje kod lekara-->
        <Button 
            Height="70"
            x:Name="kodLekaraBtn"
            Content="Zakazivanje kod određenog lekara"
            Margin="21, 10" Click="kodLekaraBtn_Click"
            FontSize="16"/>
        <!--Zakazivanje kod lekara-->
        <Button 
            Height="70"
            x:Name="uTerminuBtn"
            Content="Zakazivanje u određenom terminu"
            Margin="21,5" Click="uTerminuBtn_Click"
            FontSize="16"/>
        <!--Odustani-->
        <Button 
            Height="70"
            x:Name="izadjiBtn"
            Content="Izađi"
            Margin="21,10" Click="izadjiBtn_Click"
            FontSize="16"/>
    </StackPanel>
</Window>